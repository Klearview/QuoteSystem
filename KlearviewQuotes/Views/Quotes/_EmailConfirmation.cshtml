@model KlearviewQuotes.Models.Quote

<div class="modal fade" id="email-confirm">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Confirm Send Quote to Customer</h5>
                <button type="button" class="close" data-dismiss="modal" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <h2>Test</h2>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal" data-bs-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>